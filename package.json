{
  "name": "soshal",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.6.11",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "all": "run-s -s lint:fix test",
    "build": "SKIP_ENV_VALIDATION=1 next build",
    "dev": "./scripts/dev.sh",
    "e2e": "playwright test",
    "e2e:server": "NODE_EXTRA_CA_CERTS=./docker/mkcert/rootCA.pem run-p -l e2e:server:*",
    "e2e:server:myhost": "dotenv -e e2e/.env.myhost -- next start",
    "e2e:server:remote": "dotenv -e e2e/.env.remote -- next start -p 3001",
    "lint": "./scripts/lint.sh",
    "lint:fix": "./scripts/lint.sh --fix",
    "mutation-test": "run-s mutation-test:run-only mutation-test:report",
    "mutation-test:report": "./scripts/mutation-report.ts | md",
    "mutation-test:run-only": "./scripts/mutation-test.sh",
    "postinstall": "prisma generate",
    "prebuild": "prisma db push",
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "@heroicons/react": "2.0.18",
    "@prisma/client": "5.1.1",
    "bcryptjs": "2.4.3",
    "next": "13.4.12",
    "next-auth": "4.22.3",
    "nodemailer": "6.9.4",
    "react": "18.3.0-canary-8ec962d82-20230623",
    "react-dom": "18.3.0-canary-8ec962d82-20230623",
    "sharp": "0.32.4",
    "winston": "3.10.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "1.36.2",
    "@stryker-mutator/api": "7.1.1",
    "@stryker-mutator/core": "7.1.1",
    "@stryker-mutator/jest-runner": "7.1.1",
    "@stryker-mutator/typescript-checker": "7.1.1",
    "@tailwindcss/forms": "0.5.4",
    "@types/bcryptjs": "2.4.2",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.7",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "activitypub-core-types": "0.3.2",
    "autoprefixer": "10.4.14",
    "better-typescript-lib": "2.4.0",
    "cli-markdown": "3.0.5",
    "dotenv-cli": "7.2.1",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-mock-extended": "3.0.5",
    "lint-staged": "13.2.3",
    "msw": "1.2.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.27",
    "prettier": "3.0.1",
    "prettier-plugin-pkg": "^0.18.0",
    "prettier-plugin-sh": "^0.13.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "5.1.1",
    "shellcheck": "^2.2.0",
    "tailwindcss": "3.3.3",
    "ts-node": "10.9.1",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  }
}
