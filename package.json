{
  "name": "soshal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "./scripts/dev.sh",
    "test": "jest",
    "e2e": "playwright test",
    "e2e:server": "NODE_EXTRA_CA_CERTS=./docker/mkcert/rootCA.pem run-p -l e2e:server:*",
    "e2e:server:myhost": "dotenv -e e2e/.env.myhost -- next start -p 3001",
    "e2e:server:remote": "dotenv -e e2e/.env.remote -- next start -p 3002",
    "postinstall": "prisma generate",
    "lint": "next lint && prettier . --check",
    "lint:fix": "next lint --fix && prettier . --write --check",
    "typecheck": "tsc",
    "all": "run-s -s typecheck test lint:fix",
    "mutation-test": "run-s mutation-test:*",
    "mutation-test:run-only": "./scripts/mutation-test.sh",
    "mutation-test:report": "./scripts/mutation-report.ts | md",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "2.0.18",
    "@prisma/client": "4.15.0",
    "bcryptjs": "^2.4.3",
    "next": "canary",
    "next-auth": "4.22.1",
    "nodemailer": "6.9.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.1",
    "winston": "3.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "1.34.3",
    "@stryker-mutator/api": "7.0.2",
    "@stryker-mutator/core": "7.0.2",
    "@stryker-mutator/jest-runner": "7.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "29.5.2",
    "@types/node": "20.2.0",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "activitypub-core-types": "0.3.2",
    "autoprefixer": "10.4.14",
    "better-typescript-lib": "2.4.0",
    "cli-markdown": "3.0.3",
    "dotenv-cli": "7.2.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-mock-extended": "3.0.4",
    "lint-staged": "13.2.2",
    "msw": "1.2.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prisma": "4.15.0",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "tsx": "3.12.7",
    "typescript": "5.1.3"
  },
  "packageManager": "pnpm@8.6.1"
}
