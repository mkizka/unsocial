{
  "name": "unsocial",
  "version": "0.0.8",
  "private": true,
  "packageManager": "pnpm@8.13.1",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "_e2e:server": "UNSOCIAL_LOG_LEVEL=debug NODE_EXTRA_CA_CERTS=./docker/mkcert/rootCA.pem run-p -l _e2e:server:*",
    "_e2e:server:myhost": "dotenv -e e2e/.env.myhost -- next start",
    "_e2e:server:remote": "dotenv -e e2e/.env.remote -- next start -p 3001 || exit 0",
    "_lint:fix": "./scripts/lint.sh --fix",
    "_test:mutation:report": "./scripts/mutation-report.ts | md",
    "_test:mutation:run": "./scripts/mutation-test.sh",
    "_test:storybook:run": "wait-on tcp:9009 && test-storybook --url http://127.0.0.1:9009",
    "_test:storybook:server": "storybook dev -p 9009 --no-open > /dev/null 2>&1",
    "all": "run-s -s _lint:fix test-u test:storybook-u",
    "build": "SKIP_ENV_VALIDATION=1 next build",
    "dev": "./scripts/dev.sh",
    "e2e": "playwright test",
    "lint": "./scripts/lint.sh",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006 --quiet",
    "test": "jest",
    "test-u": "jest --updateSnapshot",
    "test:mutation": "run-s _test:mutation:run _test:mutation:report",
    "test:storybook": "run-p -sr _test:storybook:*",
    "test:storybook-u": "run-p -sr '_test:storybook:* -- --updateSnapshot'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.484.0",
    "@heroicons/react": "2.1.1",
    "@prisma/client": "5.7.1",
    "bcryptjs": "2.4.3",
    "clsx": "2.1.0",
    "jotai": "2.6.1",
    "next": "14.0.5-canary.31",
    "next-auth": "4.24.5",
    "react": "18.3.0-canary-d803f519e-20231020",
    "react-dom": "18.3.0-canary-d803f519e-20231020",
    "react-simple-image-viewer": "1.2.2",
    "sanitize-html": "2.11.0",
    "sharp": "0.32.6",
    "swiper": "11.0.5",
    "tailwind-merge": "2.2.0",
    "winston": "3.11.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@dword-design/eslint-plugin-import-alias": "4.0.9",
    "@playwright/test": "1.40.1",
    "@storybook/addon-essentials": "7.6.6",
    "@storybook/addon-interactions": "7.6.6",
    "@storybook/jest": "0.2.3",
    "@storybook/nextjs": "7.6.6",
    "@storybook/react": "7.6.6",
    "@storybook/test-runner": "0.16.0",
    "@storybook/testing-library": "0.2.2",
    "@stryker-mutator/api": "8.0.0",
    "@stryker-mutator/core": "8.0.0",
    "@stryker-mutator/jest-runner": "8.0.0",
    "@stryker-mutator/typescript-checker": "8.0.0",
    "@swc-jotai/react-refresh": "0.1.0",
    "@tailwindcss/forms": "0.5.7",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/bcryptjs": "2.4.6",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.6",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@types/sanitize-html": "2.9.5",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "activitypub-core-types": "0.3.2",
    "autoprefixer": "10.4.16",
    "cli-markdown": "3.2.0",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-eqeqeq-fix": "1.0.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "knip": "3.9.0",
    "lint-staged": "15.2.0",
    "msw": "2.0.11",
    "npm-run-all2": "6.1.1",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "prettier-plugin-pkg": "0.18.0",
    "prettier-plugin-sh": "0.13.1",
    "prettier-plugin-tailwindcss": "0.5.10",
    "prisma": "5.7.1",
    "storybook": "7.6.6",
    "tailwindcss": "3.4.0",
    "ts-node": "10.9.2",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "wait-on": "7.2.0"
  }
}
