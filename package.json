{
  "name": "unsocial",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.7.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "all": "run-s -s lint:fix test",
    "build": "./scripts/build.sh",
    "dev": "./scripts/dev.sh",
    "e2e": "playwright test",
    "e2e:server": "NODE_EXTRA_CA_CERTS=./docker/mkcert/rootCA.pem run-p -l e2e:server:*",
    "e2e:server:myhost": "dotenv -e e2e/.env.myhost -- next start",
    "e2e:server:remote": "dotenv -e e2e/.env.remote -- next start -p 3001",
    "lint": "./scripts/lint.sh",
    "lint:fix": "./scripts/lint.sh --fix",
    "mutation-test": "run-s mutation-test:run-only mutation-test:report",
    "mutation-test:report": "./scripts/mutation-report.ts | md",
    "mutation-test:run-only": "./scripts/mutation-test.sh",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "seed": "NODE_ENV=development dotenv -- ./scripts/seed.ts",
    "test": "jest"
  },
  "dependencies": {
    "@heroicons/react": "2.0.18",
    "@prisma/client": "5.2.0",
    "bcryptjs": "2.4.3",
    "next": "13.4.19",
    "next-auth": "4.23.1",
    "nodemailer": "6.9.4",
    "react": "18.3.0-canary-8ec962d82-20230623",
    "react-dom": "18.3.0-canary-8ec962d82-20230623",
    "sharp": "0.32.5",
    "superjson": "1.13.1",
    "swr": "2.2.2",
    "winston": "3.10.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@playwright/test": "1.37.1",
    "@stryker-mutator/api": "7.1.1",
    "@stryker-mutator/core": "7.1.1",
    "@stryker-mutator/jest-runner": "7.1.1",
    "@stryker-mutator/typescript-checker": "7.1.1",
    "@tailwindcss/forms": "0.5.5",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/bcryptjs": "2.4.2",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "activitypub-core-types": "0.3.2",
    "autoprefixer": "10.4.15",
    "cli-markdown": "3.0.5",
    "dotenv-cli": "7.3.0",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-mock-extended": "3.0.5",
    "lint-staged": "14.0.1",
    "msw": "1.2.5",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.28",
    "prettier": "3.0.2",
    "prettier-plugin-pkg": "0.18.0",
    "prettier-plugin-sh": "0.13.1",
    "prettier-plugin-tailwindcss": "0.5.3",
    "prisma": "5.2.0",
    "shellcheck": "2.2.0",
    "tailwindcss": "3.3.3",
    "ts-node": "10.9.1",
    "tsx": "3.12.7",
    "typescript": "5.2.2"
  }
}
