{
  "name": "unsocial",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.10.2",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "all": "run-s -s lint:fix test test-storybook",
    "build": "SKIP_ENV_VALIDATION=1 next build",
    "build-storybook": "storybook build",
    "dev": "./scripts/dev.sh",
    "dev:mastodon": "./scripts/dev.sh mastodon",
    "dev:misskey": "./scripts/dev.sh misskey",
    "e2e": "playwright test",
    "e2e:server": "NODE_EXTRA_CA_CERTS=./docker/mkcert/rootCA.pem run-p -l e2e:server:*",
    "e2e:server:myhost": "dotenv -e e2e/.env.myhost -- next start",
    "e2e:server:remote": "dotenv -e e2e/.env.remote -- next start -p 3001 || exit 0",
    "lint": "./scripts/lint.sh",
    "lint:fix": "./scripts/lint.sh --fix",
    "mutation-test": "run-s mutation-test:run-only mutation-test:report",
    "mutation-test:report": "./scripts/mutation-report.ts | md",
    "mutation-test:run-only": "./scripts/mutation-test.sh",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "seed": "NODE_ENV=development dotenv -- ./scripts/seed.ts",
    "storybook": "storybook dev -p 6006 --quiet --no-open",
    "test": "jest",
    "test-storybook": "run-p -sr test-storybook:*",
    "test-storybook:run": "TARGET_URL=http://127.0.0.1:9009 bash -c 'wait-on $TARGET_URL && test-storybook'",
    "test-storybook:server": "storybook dev -p 9009 --no-open > /dev/null 2>&1"
  },
  "dependencies": {
    "@heroicons/react": "2.0.18",
    "@prisma/client": "5.5.2",
    "bcryptjs": "2.4.3",
    "clsx": "2.0.0",
    "jotai": "2.5.0",
    "next": "14.0.2-canary.6",
    "next-auth": "4.24.4",
    "nodemailer": "6.9.7",
    "react": "18.3.0-canary-d803f519e-20231020",
    "react-dom": "18.3.0-canary-d803f519e-20231020",
    "react-simple-image-viewer": "1.2.2",
    "sharp": "0.32.6",
    "tailwind-merge": "2.0.0",
    "winston": "3.11.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@dword-design/eslint-plugin-import-alias": "4.0.8",
    "@playwright/test": "1.39.0",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/jest": "0.2.3",
    "@storybook/nextjs": "7.5.3",
    "@storybook/react": "7.5.3",
    "@storybook/test-runner": "0.14.0",
    "@storybook/testing-library": "0.2.2",
    "@stryker-mutator/api": "7.3.0",
    "@stryker-mutator/core": "7.3.0",
    "@stryker-mutator/jest-runner": "7.3.0",
    "@stryker-mutator/typescript-checker": "7.3.0",
    "@swc-jotai/react-refresh": "0.1.0",
    "@tailwindcss/forms": "0.5.6",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/bcryptjs": "2.4.5",
    "@types/jest": "29.5.7",
    "@types/node": "20.8.10",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "activitypub-core-types": "0.3.2",
    "autoprefixer": "10.4.16",
    "cli-markdown": "3.0.5",
    "dotenv-cli": "7.3.0",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "eslint-plugin-eqeqeq-fix": "1.0.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "lint-staged": "15.0.2",
    "msw": "2.0.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-pkg": "0.18.0",
    "prettier-plugin-sh": "0.13.1",
    "prettier-plugin-tailwindcss": "0.5.6",
    "prisma": "5.5.2",
    "storybook": "7.5.3",
    "tailwindcss": "3.3.5",
    "ts-node": "10.9.1",
    "tsx": "3.14.0",
    "typescript": "5.2.2",
    "wait-on": "7.1.0"
  }
}
