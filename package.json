{
  "name": "unsocial",
  "version": "0.0.30",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "_down:jest": "docker compose -f ./jest/compose.yaml down",
    "_down:root": "docker compose down -v --remove-orphans",
    "_e2e:server": "UNSOCIAL_LOG_LEVEL=debug NODE_EXTRA_CA_CERTS=./docker/mkcert/rootCA.pem run-p -l _e2e:server:*",
    "_e2e:server:myhost": "dotenv -e e2e/.env.myhost -- next start",
    "_e2e:server:remote": "dotenv -e e2e/.env.remote -- next start -p 3001 || exit 0",
    "_lint:fix": "./scripts/lint.sh --fix",
    "_test:mutation:report": "./scripts/mutation-report.ts | md",
    "_test:mutation:run": "dotenv -e .env.test -- ./scripts/mutation-test.sh",
    "_test:storybook:run": "wait-on tcp:9009 && test-storybook --url http://127.0.0.1:9009",
    "_test:storybook:server": "storybook dev -p 9009 --no-open > /dev/null 2>&1",
    "all": "run-s -s _lint:fix test-u test:storybook-u",
    "build": "SKIP_ENV_VALIDATION=1 next build",
    "dev": "./scripts/dev.sh",
    "down": "run-s -s _down:*",
    "e2e": "playwright test",
    "lint": "./scripts/lint.sh",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006 --quiet",
    "test": "jest",
    "test-u": "pnpm test -- --updateSnapshot",
    "test:mutation": "run-s -s _test:mutation:run _test:mutation:report",
    "test:storybook": "run-p -sr _test:storybook:*",
    "test:storybook-u": "run-p -sr '_test:storybook:* -- --updateSnapshot'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.507.0",
    "@heroicons/react": "2.1.1",
    "@prisma/client": "5.9.1",
    "@sentry/nextjs": "7.99.0",
    "async-mutex": "0.4.1",
    "bcryptjs": "2.4.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "jotai": "2.6.4",
    "next": "14.1.1-canary.36",
    "next-auth": "4.24.5",
    "react": "18.3.0-canary-feed8f3f9-20240118",
    "react-dom": "18.3.0-canary-feed8f3f9-20240118",
    "react-simple-image-viewer": "1.2.2",
    "sanitize-html": "2.11.0",
    "sharp": "0.32.6",
    "swiper": "11.0.6",
    "tailwind-merge": "2.2.1",
    "zod": "3.22.4",
    "zod-validation-error": "3.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@dword-design/eslint-plugin-import-alias": "4.0.9",
    "@playwright/test": "1.41.2",
    "@quramy/jest-prisma-core": "1.7.3",
    "@quramy/jest-prisma-node": "1.7.3",
    "@quramy/prisma-fabbrica": "2.1.0",
    "@storybook/addon-essentials": "7.6.12",
    "@storybook/addon-interactions": "7.6.12",
    "@storybook/jest": "0.2.3",
    "@storybook/nextjs": "7.6.12",
    "@storybook/react": "7.6.12",
    "@storybook/test-runner": "0.16.0",
    "@storybook/testing-library": "0.2.2",
    "@stryker-mutator/api": "8.2.3",
    "@stryker-mutator/core": "8.2.3",
    "@stryker-mutator/jest-runner": "8.2.3",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/bcryptjs": "2.4.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.16",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.18",
    "@types/sanitize-html": "2.11.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "autoprefixer": "10.4.17",
    "cli-markdown": "3.2.2",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-eqeqeq-fix": "1.0.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-tailwindcss": "3.14.2",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "9.0.10",
    "jest": "29.7.0",
    "jest-diff": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "knip": "4.4.0",
    "lint-staged": "15.2.2",
    "msw": "2.1.7",
    "npm-run-all2": "6.1.2",
    "postcss": "8.4.34",
    "prettier": "3.2.5",
    "prettier-plugin-pkg": "0.18.1",
    "prettier-plugin-sh": "0.14.0",
    "prisma": "5.9.1",
    "storybook": "7.6.12",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "1.0.7",
    "ts-node": "10.9.2",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "wait-on": "7.2.0"
  }
}
