{
  "name": "soshal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "SKIP_ENV_VALIDATION=1 next build && tsc --project tsconfig.server.json",
    "dev": "docker compose up -d mysql mail && nodemon",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "jest",
    "e2e": "playwright test",
    "postinstall": "prisma generate",
    "lint": "next lint && prettier . --check",
    "mutation-test": "run-s mutation-test:*",
    "mutation-test:run-only": "./scripts/mutation-test.sh",
    "mutation-test:report": "./scripts/mutation-report.ts | md",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "canary"
  },
  "devDependencies": {
    "@heroicons/react": "2.0.18",
    "@next-auth/prisma-adapter": "1.0.6",
    "@playwright/test": "1.34.3",
    "@prisma/client": "4.14.1",
    "@stryker-mutator/api": "6.4.2",
    "@stryker-mutator/core": "6.4.2",
    "@stryker-mutator/jest-runner": "6.4.2",
    "@types/jest": "29.5.1",
    "@types/node": "20.2.0",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "activitypub-core-types": "0.3.2",
    "autoprefixer": "10.4.14",
    "better-typescript-lib": "2.3.1",
    "cli-markdown": "3.0.3",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "got": "11.8.6",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-mock-extended": "3.0.4",
    "lint-staged": "13.2.2",
    "next-auth": "4.19.2",
    "nock": "13.3.1",
    "nodemailer": "6.9.2",
    "nodemon": "^2.0.22",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "prisma": "4.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "tsx": "3.12.7",
    "typescript": "5.0.4",
    "winston": "3.9.0",
    "zod": "3.21.4"
  },
  "packageManager": "pnpm@8.5.1",
  "pnpm": {
    "patchedDependencies": {
      "next-auth@4.19.2": "patches/next-auth@4.19.2.patch"
    }
  }
}
